{
  "name": "office-days",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "type-check": "tsc",
    "test": "vitest --changed origin/main",
    "test:all": "vitest run",
    "test:setup": "npx playwright install chromium --with-deps",
    "db": "drizzle-kit --config ./scripts/drizzle-kit.config.ts",
    "db:migrate": "npx tsx ./scripts/migrate.ts",
    "db:supabase-types": "npx supabase gen types --lang=typescript --project-id 'hlbhxwwxawelqgmgmsoa' --schema public > ./src/db/supabase-types.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@conform-to/zod": "^1.2.2",
    "@kobalte/core": "^0.13.7",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.7",
    "@solidjs/start": "^1.0.8",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.25.0",
    "drizzle-orm": "^0.34.1",
    "lucide-solid": "^0.451.0",
    "luxon": "^3.5.0",
    "postgres": "^3.4.4",
    "solid-js": "^1.9.1",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "^0.4.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@vitest/browser": "^2.1.2",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "eslint": "^9.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "playwright": "^1.47.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.1.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --write --ignore-unknown"
  }
}
